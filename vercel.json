{
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/checkout/checkout.js",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/checkout/checkout.css",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/checkout/themes/:path*",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/images/:path*",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/home.html" },
    { "source": "/home", "destination": "/home.html" },
    { "source": "/home/", "destination": "/home.html" },
    { "source": "/api/admin/:path*", "destination": "/api/admin/[[...path]]?path=:path*" },
    { "source": "/api/dashboard/:path*", "destination": "/api/dashboard/[[...path]]?path=:path*" },
    { "source": "/api/create-pix", "destination": "/api/create-pix4" },
    { "source": "/api/shipping-methods", "destination": "/api/dashboard/shipping-methods" },
    { "source": "/api/shipping-methods/:id", "destination": "/api/dashboard/shipping-methods/:id" },
    { "source": "/api/dashboard/upload/logo", "destination": "/api/dashboard/upload?id=logo" },
    { "source": "/api/admin/upload/logo", "destination": "/api/admin/upload?id=logo" },
    { "source": "/signup.html", "destination": "/admin/signup.html" },
    { "source": "/login.html", "destination": "/dashboard/index.html" },
    { "source": "/signup", "destination": "/admin/signup.html" },
    { "source": "/login", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/builder", "destination": "/dashboard/builder.html" },
    { "source": "/dashboard/builder/", "destination": "/dashboard/builder.html" },
    { "source": "/dashboard/:tab", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/:tab/", "destination": "/dashboard/index.html" },
    { "source": "/dashboard", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/", "destination": "/dashboard/index.html" },
    { "source": "/admin", "destination": "/admin/dashboard.html" },
    { "source": "/admin/", "destination": "/admin/dashboard.html" },
    { "source": "/checkout/:slug", "destination": "/checkout/index.html" },
    { "source": "/checkout/:slug/", "destination": "/checkout/index.html" },
    { "source": "/checkout/:slug/:rest*", "destination": "/checkout/index.html" }
  ]
}
